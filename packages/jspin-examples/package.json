{
  "name": "@jspin/examples",
  "version": "0.1.0",
  "description": "Example implementations using JSpin core library",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/basic-example.ts",
    "start:basic": "ts-node src/basic-example.ts",
    "start:switch": "ts-node src/switch-example.ts", 
    "start:led": "ts-node src/led-animation-example.ts",
    "dev": "ts-node --watch src/basic-example.ts",
    "dev:basic": "ts-node --watch src/basic-example.ts",
    "dev:switch": "ts-node --watch src/switch-example.ts",
    "dev:led": "ts-node --watch src/led-animation-example.ts"
  },
  "dependencies": {
    "@jspin/core": "file:../jspin-core",
    "color": "^5.0.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
